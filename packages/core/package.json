{
  "name": "@json-visual-diff/core",
  "version": "0.0.1",
  "description": "核心差异算法引擎，提供高性能的 JSON 对象比较功能，支持智能数组对比、扩展类型和循环引用检测",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "json",
    "diff",
    "compare",
    "algorithm"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "fast-check": "^3.22.0",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
